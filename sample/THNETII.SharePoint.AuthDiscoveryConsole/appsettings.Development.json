{
  "Microsoft.Identity.Client": {
    "EnablePiiLogging": true
  },
  "Logging": {
    "LogLevel": {
      "Microsoft.Identity.Client": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "System.Net.Http.HttpClient.*.ClientHandler": "Information"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Lifetime": {
    "SuppressStatusMessages": false
  }
}
